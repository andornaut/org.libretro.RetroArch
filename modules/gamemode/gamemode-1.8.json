{
    "name": "Gamemode",
    "buildsystem": "meson",
    "config-opts": [
      "-Dwith-examples=false",
      "-Dwith-util=false",
      "-Dwith-sd-bus-provider=no-daemon"
    ],
    "sources": [
      {
        "type": "archive",
        "url": "https://github.com/FeralInteractive/gamemode/archive/refs/tags/1.8.1.tar.gz",
        "sha256": "cdb117d05d65dbd03c0bd2104df874a1f878db8f23cc4d55bfa44e832482269e"
      }
    ],
    "cleanup": [ "/include", "/lib/pkgconfig" ],
    "post-install": [
      "install -t /app/bin -Dm755 ../data/gamemoderun",
      "rm /app/lib/libgamemodeauto.a"
    ]
}